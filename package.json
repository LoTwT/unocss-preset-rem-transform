{
  "name": "unocss-preset-rem-transform",
  "version": "0.0.0",
  "packageManager": "pnpm@7.9.5",
  "description": "unocss-preset-rem-transform",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild",
    "lint": "prettier -c .",
    "prettier": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "prepare": "simple-git-hooks",
    "up": "taze latest -I",
    "play": "pnpm -C playground run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/unocss-preset-rem-transform.git"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "rem",
    "rem-transform"
  ],
  "author": "LoTwT <709937065@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LoTwT/unocss-preset-rem-transform/issues"
  },
  "homepage": "https://github.com/LoTwT/unocss-preset-rem-transform#readme",
  "devDependencies": {
    "@types/node": "^18.7.13",
    "bumpp": "^8.2.1",
    "lint-staged": "^13.0.3",
    "pnpm": "^7.9.5",
    "prettier": "^2.7.1",
    "simple-git-hooks": "^2.8.0",
    "taze": "^0.8.0",
    "typescript": "^4.8.2",
    "unbuild": "^0.8.9",
    "unocss": "^0.45.13",
    "unplugin-auto-import": "^0.11.2",
    "vitest": "^0.22.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
